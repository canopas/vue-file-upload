{
  "name": "@canopassoftware/vue-file-upload",
  "version": "1.0.4",
  "description": "Show the preview of file and manage files data to upload",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canopas/file-upload-web-frontend.git"
  },
  "keywords": [
    "file-upload",
    "image-upload",
    "file-management",
    "file-preview",
    "image-preview"
  ],
  "author": "dharti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/canopas/file-upload-web-frontend/issues"
  },
  "homepage": "https://github.com/canopas/file-upload-web-frontend#readme",
  "scripts": {
    "dev": "tailwindcss -i ./src/input.css -o ./dist/output.css && vite",
    "build": "vite build && tailwindcss -i ./src/input.css -o ./dist/output.css --minify && cp ./src/assets/style.scss ./dist/style.scss",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "sass": "^1.69.4",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.11"
  },
  "directories": {
    "example": "examples"
  }
}
